@prefix : <http://data.gmdsp.org.uk/def/council/neighbourhood/allotment#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix qudt: <http://qudt.org/vocab/unit> .
@prefix foaf: <http://xmlns.com/foaf/0.1/>.

#################################################################
#
#    General Information
#
#################################################################

<http://data.gmdsp.org.uk/def/council/neighbourhood/allotment> rdf:type owl:Ontology ;
                                  
                                  rdfs:label "Ontology which describes features of an allotment"^^xsd:string ;
                                  
                                  owl:versionInfo "Draft"^^xsd:string .

### Classes

:Allotment a owl:Class .
                                  


#################################################################
#
#    Object Properties
#
#################################################################

:rent rdf:type owl:ObjectProperty ;
                 
	rdfs:label "rent"^^xsd:string ;
                 
	rdfs:comment "The rent for the allotment" ;

	rdfs:domain :Allotment ;
	
	rdfs:range xsd:integer .

:number-of-plots rdf:type owl:ObjectProperty ;
            
	rdfs:label "number of plots"^^xsd:string ;
            
    rdfs:comment "Number of plots which the allotment has"^^xsd:string ;

	rdfs:domain :Allotment ;
    
	rdfs:range xsd:integer .


:disabled-access rdf:type owl:ObjectProperty ;
                
	rdfs:label "disabled access"^^xsd:string ;
                
	rdfs:domain :Allotment ;
    
	rdfs:comment "Does the allotment have disabled access"^^xsd:string ;

	rdfs:range xsd:boolean .


:plot-size rdf:type owl:ObjectProperty ;
          
	rdfs:label "plot size"^^xsd:string ;
          
	rdfs:comment "How big is the allotment plot"^^xsd:string ;

	rdfs:domain :Allotment ;

	rdfs:range qudt:squareMeter .
          

:guidance-page rdf:type owl:ObjectProperty ;
        
	rdfs:label "has guidance page"^^xsd:string ;
        
	rdfs:domain :Allotment ;        

	rdfs:comment "Page (given by URL) with guidance for this allotment."^^xsd:string ;

	rdfs:subPropertyOf foaf:page .

		
:application-page rdf:type owl:ObjectProperty ;

	rdfs:domain :Allotment ;

	rdfs:label "has application page"^^xsd:string ;

	rdfs:comment "Page (given by URL) with application information for this allotment."^^xsd:string ;

	rdfs:subPropertyOf foaf:page .
		

:information-page rdf:type owl:ObjectProperty ;

	rdfs:domain :Allotment ;

	rdfs:label "has information page"^^xsd:string ;

	rdfs:comment "Page (given by URL) where further information for this allotment."^^xsd:string ;

	rdfs:subPropertyOf foaf:page .