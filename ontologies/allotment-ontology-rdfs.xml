		<rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Each allotment can have a number of plots, each of which can be uniquely defined</rdfs:comment>
		<rdfs:domain rdf:resource="http://data.gmdsp.org.uk/def/council/Allotment" />
		<rdfs:range>
			<rdf:Description rdf:about="http://data.gmdsp.org.uk/def/council/Plot">
				<rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Part of an allotment which has been split into smaller areas</rdfs:label>
			</rdf:Description>
		</rdfs:range>
	</rdf:Description>
	<rdfs:Property rdf:about="http://data.gmdsp.org.uk/def/council/allotment/disabled-access">
		<rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Disabled access</rdfs:label>
		<rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Does the allotment have disabled access</rdfs:comment>
		<rdfs:domain rdf:resource="http://data.gmdsp.org.uk/def/council/Allotment" />
		<rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean" />
	</rdfs:Property>
	<rdfs:Property rdf:about="http://data.gmdsp.org.uk/def/council/allotment/plot-size">
		<rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Plot size</rdfs:label>
		<rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">How big is the allotment plot in square meters</rdfs:comment>
		<rdfs:domain rdf:resource="http://data.gmdsp.org.uk/def/council/Allotment" />
		<rdfs:range rdf:resource="http://qudt.org/vocab/unit/squareMeter" />
	</rdfs:Property>
