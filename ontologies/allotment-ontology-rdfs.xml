<?xml version="1.0"?>
<rdf:RDF xmlns:allotment="http://data.gmdsp.org.uk/def/council/allotment/" xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:gr="http://purl.org/goodrelations/v1#" xmlns:xsd="http://www.w3.org/2001/XMLSchema#" xmlns:foaf="http://xmlns.com/foaf/0.1/" xmlns:council="http://data.gmdsp.org.uk/def/council/" xmlns:qudt="http://qudt.org/vocab/unit/" xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">
	<rdf:Description rdf:about="http://data.gmdsp.org.uk/def/council/Allotment">
