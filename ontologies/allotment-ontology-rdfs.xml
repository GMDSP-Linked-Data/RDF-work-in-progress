		<rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Number of plots which the allotment has</rdfs:comment>
		<rdfs:domain rdf:resource="http://data.gmdsp.org.uk/def/council/Allotment" />
		<rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer" />
	</rdfs:Property>
